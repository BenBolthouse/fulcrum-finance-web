{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "test": "nx run infrastructure:test",
    "lint": "nx run infrastructure:lint"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "aws-cdk": "2.150.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.150.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "nx": {
    "targets": {
      "lint": {
        "executor": "@nx/eslint:lint",
        "options": {
          "lintFilePatterns": [
            "infrastructure/**/*.{ts,tsx,js,jsx}"
          ]
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "options": {
          "jestConfig": "infrastructure/jest.config.js",
          "passWithNoTests": true
        }
      }
    }
  }
}
