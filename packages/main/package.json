{
    "name": "@fulcrum-finance/main",
    "main": "build/index.js",
    "type": "module",
    "scripts": {
        "build": "nx run main:build",
        "test": "nx run main:test",
        "lint": "nx run main:lint"
    },
    "keywords": [],
    "author": "https://github.com/benbolthouse",
    "license": "MIT",
    "nx": {
        "targets": {
            "build": {
                "executor": "@nx/vite:build",
                "options": {
                    "outputPath": "packages/main/build",
                    "configFile": "packages/main/vite.config.ts",
                    "main": "packages/main/index.ts",
                    "tsConfig": "packages/main/tsconfig.json"
                }
            },
            "lint": {
                "executor": "@nx/eslint:lint",
                "options": {
                    "lintFilePatterns": [
                        "packages/main/**/*.{ts,tsx,js,jsx}"
                    ]
                }
            },
            "test": {
                "executor": "@nx/jest:jest",
                "options": {
                    "jestConfig": "packages/main/jest.config.js",
                    "passWithNoTests": true
                }
            }
        }
    },
    "devDependencies": {
        "@nx/vite": "^21.3.11",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "@vitejs/plugin-react": "^4.7.0",
        "vite": "^6.3.5"
    },
    "dependencies": {
        "consola": "^3.4.2",
        "oidc-client-ts": "^3.3.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    }
}
